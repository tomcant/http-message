{
    "name": "tomcant/http-message",
    "description": "Simple HTTP message implementation that conforms (eventually) with PSR-7 and PSR-17.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Cant",
            "email": "tomcant@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "http-interop/http-factory-tests": "^0.6.0",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "SimpleWeb\\Http\\Message\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SimpleWeb\\Http\\Message\\": "tests/"
        }
    }
}
